<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="adu.ac.ae.elevatorcontrolpanelapplication">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon3"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon3_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ElevatorNumberThreeActivity"/>
        <activity android:name=".ElevatorNumberTwoActivity" />
        <activity android:name=".ElevatorNumberOneActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />
    </application>

</manifest>